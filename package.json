{
    "name": "@tractorzoom/component-library",
    "description": "Component library using Material UI",
    "author": "@TractorZoom",
    "license": "MIT",
    "engines": {
        "node": "12.x"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/TractorZoom/component-library.git"
    },
    "homepage": "https://github.com/TractorZoom/component-library",
    "bugs": {
        "url": "https://github.com/TractorZoom/component-library/issues"
    },
    "keywords": [
        "material-ui",
        "theme"
    ],
    "scripts": {
        "build-storybook": "build-storybook -s public",
        "build": "npm run build-storybook",
